                    "application/json"
                ],
                "description": "简单随机任务",
                "parameters": [
                    {
                        "in": "body",
                        "name": "path",
                        "description": "抽样文件路径",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "example": "hdfs://localhost:9000/dataset/titanic/train.csv"
                        }
                    },
                    {
                        "in": "body",
                        "name": "method",
                        "description": "抽样方法，根据engine映射",
