        "/evaluation/statistics/": {
            "post": {
                "tags": [
                    "evaluation"
                ],
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "description": "统计任务",
                "parameters": [
                    {
                        "in": "body",
                        "name": "path",
                        "description": "统计文件路径，与job_id选一",
                        "schema": {
                            "type": "string",
                            "example": "hdfs://localhost:9000/dataset/titanic/train.csv"
                        }
                    },
                    {
                        "in": "body",
                        "name": "job_id",
                        "description": "抽样任务id（用于获取抽样后文件进行统计，与path选一）",
                        "schema": {
                            "type": "integer",
                            "example": "10021"
                        }
                    },
                    {
                        "in": "body",
                        "name": "method",
                        "description": "统计方法，根据engine映射",
                        "schema": {
                            "$ref": "#/definitions/statistics_method"
                        }
                    },
                    {
                        "in": "body",
                        "name": "type",
                        "description": "输入的抽样文件格式",
                        "schema": {
                            "$ref": "#/definitions/file_type"
                        }
                    },
                    {
                        "in": "body",
                        "name": "with_header",
                        "description": "输入文件是否带头，仅对csv有效",
                        "schema": {
                            "type": "boolean",
                            "default": true
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/statistics_job_response"
                        }
                    }
                }
            }
        },
        "/evaluation/job/": {
            "post": {
                "tags": [
                    "evaluation"
                ],
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "description": "评估任务",
                "parameters": [
                    {
                        "in": "body",
                        "name": "path",
                        "description": "评估文件路径",
                        "schema": {
                            "type": "string",
                            "example": "hdfs://localhost:9000/dataset/titanic/train.csv-sampled-1616428937.70054"
                        }
                    },
                    {
                        "in": "body",
                        "name": "source_path",
                        "description": "源文件（采样前）路径，用于对比评估",
                        "schema": {
                            "type": "string",
                            "example": "hdfs://localhost:9000/dataset/titanic/train.csv"
                        }
                    },
                    {
                        "in": "body",
                        "name": "compare_job_id",
                        "description": "对比评估时采样任务id",
                        "schema": {
                            "type": "integer",
                            "example": 10051
                        }
                    },
                    {
                        "in": "body",
                        "name": "method",
                        "description": "评估方法，根据engine映射",
                        "schema": {
                            "$ref": "#/definitions/evaluation_method"
                        }
                    },
                    {
                        "in": "body",
                        "name": "type",
                        "description": "输入的抽样文件格式",
                        "schema": {
                            "$ref": "#/definitions/file_type"
                        }
                    },
                    {
                        "in": "body",
                        "name": "with_header",
                        "description": "输入文件是否带头，仅对csv有效",
                        "schema": {
                            "type": "boolean",
                            "default": true
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/statistics_job_response"
                        }
                    }
                }
            }
        }
