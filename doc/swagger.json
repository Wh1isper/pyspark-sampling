    "definitions": {
        "simplejob_method": {
            "type": "string",
            "enum": [
                "random",
                "stratified"
            ]
        },
        "mljob_method": {
            "type": "string",
            "enum": [
                "spark_smote",
                "spark_enn",
                "spark_smote_enn",
                "imb_smote",
                "imb_enn",
                "imb_smote_enn"
            ]
        },
        "statistics_method": {
            "type": "string",
            "enum": [
                "basic"
            ]
        },
        "evaluation_method": {
            "type": "string",
            "enum": [
                "compare"
            ]
        },
        "file_type": {
            "type": "string",
            "enum": [
                "csv",
                "txt"
            ],
            "default": "txt"
        },
        "simplejob_conf": {
            "type": "object",
            "properties": {
                "fraction": {
                    "type": "integer",
                    "description": "采样率",
                    "default": 0.5
                },
                "seed": {
                    "type": "integer",
                    "description": "随机种子"
                },
                "with_replacement": {
                    "type": "boolean",
                    "description": "是否可重复抽取（true: 放回抽样）",
                    "default": true
                },
                "key": {
                    "type": "string",
                    "description": "分层抽样的key"
                }
            }
        },
        "response": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "description": "状态码，见code.py"
                },
                "msg": {
                    "type": "string",
                    "description": "返回信息，一般用于错误提示"
                },
                "data": {
                    "type": "object",
                    "description": "返回数据"
                }
            }
        },
        "ml_conf": {
            "type": "object",
            "properties": {
                "k_neighbors": {
                    "type": "integer",
                    "description": "(SMOTE) k近邻",
                    "default": 3
                },
                "n_neighbors": {
                    "type": "integer",
                    "description": "(ENN) n近邻",
                    "default": 3
                },
                "drop_list": {
                    "type": "array",
                    "description": "与实验无关、需要移除的列",
                    "default": []
                },
                "key": {
                    "type": "string",
                    "description": "分类标签"
                }
            }
        },
